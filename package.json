{
	"name": "specflow-binder",
	"displayName": "specflow binder",
	"description": "Gherkin Binder for Specflow (C#_",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"publisher": "amillard98",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.specbind.discoverBindings",
				"title": "Spec-Bind: Refresh Bindings"
			}
		],
		"keybindings": [
			{
				"key": "F12",
				"command": "extension.specbind.goToStep"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"line-reader": "^0.4.0"
	}
}
